}

@incollection{johnsonAtmosphericDepositionForest1993,
  title = {Atmospheric {{Deposition}}, {{Forest Nutrient Status}}, and {{Forest Decline}}: {{Implications}} of the {{Integrated Forest Study}}},
  shorttitle = {Atmospheric {{Deposition}}, {{Forest Nutrient Status}}, and {{Forest Decline}}},
  booktitle = {Forest {{Decline}} in the {{Atlantic}} and {{Pacific Region}}},
  author = {Johnson, D. W. and Lindberg, S. E. and Van Miegroet, H. and Lovett, G. M. and Cole, D. W. and Mitchell, M. J. and Binkley, D.},
