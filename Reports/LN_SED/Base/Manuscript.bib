@article{poszwaDeepTreeRoots2002,
  title = {Do Deep Tree Roots Provide Nutrients to the Tropical Rainforest?},
  author = {Poszwa, Anne and Dambrine, Etienne and Ferry, Bruno and Pollier, Beno{\^i}t and Loubet, Michel},
  year = {2002},
