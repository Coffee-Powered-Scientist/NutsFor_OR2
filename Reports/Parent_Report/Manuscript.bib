@inproceedings{saraivaCalculationSensitivityIndex2017,
  title = {Calculation of Sensitivity Index Using One-at-a-Time Measures Based on Graphical Analysis},
  booktitle = {2017 18th {{International Scientific Conference}} on {{Electric Power Engineering}} ({{EPE}})},
  author = {Saraiva, J. P. and Lima, B. S. and Gomes, V. M. and Flores, P. H. R. and Gomes, F. A. and Assis, A. O. and Reis, M. R. C. and Ara{\'u}jo, W. R. H. and Abrenhosa, C. and Calixto, W. P.},
