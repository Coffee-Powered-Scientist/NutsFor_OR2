@article{hansenIncidenceImpactSwiss2000,
  title = {Incidence and {{Impact}} of {{Swiss Needle Cast}} in {{Forest Plantations}} of {{Douglas}}-Fir in {{Coastal Oregon}}},
  author = {Hansen, E. M. and Stone, J. K. and Capitano, B. R. and Rosso, P. and Sutton, W. and Winton, L. and Kanaskie, A. and McWilliams, M. G.},
