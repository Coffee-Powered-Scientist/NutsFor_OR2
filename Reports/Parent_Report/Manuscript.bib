@article{katouDeterminingCompetitiveNitrate2004,
  title = {Determining Competitive Nitrate and Chloride Adsorption in an Andisol by the Unsaturated Transient Flow Method},
  author = {Katou, Hidetaka},
  year = {2004},
  month = feb,
  volume = {50},
