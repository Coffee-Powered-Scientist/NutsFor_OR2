@incollection{homannRelationshipsTemperateForest1992,
  title = {Relationships among {{N}}, {{P}}, and {{S}} in {{Temperate Forest Ecosystems}}},
  booktitle = {Atmospheric {{Deposition}} and {{Forest Nutrient Cycling}}: {{A Synthesis}} of the {{Integrated Forest Study}}},
  author = {Homann, P. S. and Harrison, R. B.},
