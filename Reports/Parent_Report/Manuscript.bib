  year = {1993},
  pages = {66--81},
  publisher = {{Springer}},
  address = {{Berlin, Heidelberg}},
  doi = {10.1007/978-3-642-76995-5_5},
  abstract = {The discovery of acidic deposition caused concern that forest nutritional problems and forest growth decline would result from increased base cation leaching from soils. This concern stimulated many analyses of cation budgets in forest ecosystems (see review by Johnson et al. 1982), including detailed analyses of H+ deposition vs internal H+ production (e.g., Ulrich 1980; Binkley and Richter, 1987). H+ budgets highlighted the great importance of tree uptake in causing soil acidification, but even when this was included in the analysis, budget analyses usually indicated little possibility for soil acidification or cation nutrient depletion in anything less than many decades or centuries (McFee 1980; Johnson et al. 1985). However, recent field evidence shows that some soils can experience significant changes in only one to two decades (e.g., Berden et al. 1987; Johnson et al. 1988; Binkley et al. 1989). As the issue of acidic deposition and forest decline unfolds, the importance of nutrient cycling analysis in understanding the cause-effect relationships in this phenomenon has also become very clear (Ulrich 1980; Johnson et al. 1985, 1991; Schulze 1989).},
  file = {C\:\\Users\\siahl\\Zotero\\storage\\FFUYKN4E\\Johnson et al. - 1993 - Atmospheric Deposition, Forest Nutrient Status, an.pdf},
  isbn = {978-3-642-76995-5},
  keywords = {Atmospheric Deposition,Base Cation,Bulk Deposition,Forest Decline,Nutrient Cycling},
  language = {en}
}

@incollection{johnsonBaseCations1992,
