  author = {Taylor, G. E. and {Ross-Todd}, B. M. and Allen, E. and Conklin, P. and Edmonds, R. and Joranger, E. and Miller, E. and Ragsdale, L. and Shepard, J. and Silsbee, D. and Swank, W.},
