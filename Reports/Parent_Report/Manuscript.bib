@article{colemanNutritionDouglasfirInland2014,
  title = {Nutrition of {{Douglas}}-Fir in the {{Inland Northwest}}},
  author = {Coleman, Mark D. and Shaw, Terry M. and Kimsey, Mark J. and Moore, James A.},
  year = {2014},
  volume = {78},
