  keywords = {Atmospheric Deposition,Base Cation,Canopy Leaching,Exchangeable Base Cation,Total Atmospheric Deposition},
  language = {en},
  series = {Ecological {{Studies}}}
}

@article{johnsonEffectsLimingSoils1995,
  title = {Effects of {{Liming}} on {{Soils}} and {{Streamwaters}} in a {{Deciduous Forest}}: {{Comparison}} of {{Field Results}} and {{Simulations}}},
  shorttitle = {Effects of {{Liming}} on {{Soils}} and {{Streamwaters}} in a {{Deciduous Forest}}},
