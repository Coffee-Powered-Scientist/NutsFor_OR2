  author = {Bockheim, J. G. and Langley-Turnbaugh, S.},
  year = {1997},
  volume = {26},
  pages = {292--301},
  issn = {1537-2537},
  doi = {10.2134/jeq1997.00472425002600010041x},
  abstract = {Biogeochemical cycling was studied in Sitka spruce [Picea sitchensis (Bong.) Carr.] and western hemlock-Douglas-fir [Tsuga heterophylla (Raf.) Sarg.-Pseudotsuga menziesii (Mirbel) Franco] ecosystems on chronosequence of five uplifted marine terraces in coastal Oregon. The terraces occur 0.25 to 8.6 km from the Pacific Ocean and range from 80 to {$\sim$}500 kyr in age. The soils are highly weathered and include Inceptisols on the youngest terrace and Spodosols with clay-enriched horizons and Ultisols on the older terraces. Bulk precipitation, throughfall, and soil solutions (four depths) were collected monthly from November 1992 through April 1993. The solutions were analyzed for pH, dissolved cations (Na, Mg, Ca, K, Fe, Al, and Si), anions (Cl, SO4, HCO3, NO3, and PO4), and organic C. Cations (\textmu molc L-1) in all solutions were ranked: Na {$>$} Mg {$>$} Ca {$\geq$} K {$>$} H; anions were ranked: Cl {$>$} SO4 {$>$} NO3 {$>>$} HCO3. The Na/Cl ratio in all solutions was comparable to that of seawater. The mean sums of cations and anions in all solutions declined sharply with distance from the coast. Throughfall enrichment of all ions suggests that aerosol impaction of sea salts is a dominant process within the coastal fogbelt of Oregon. In that the soils have low inherent fertility, the trees appear to obtain Ca, Mg, K, and S primarily from atmospheric sources. These nutrients are utilized by abundant fine roots in the forest floor and upper 5 to 20 cm of mineral soil. Net cation loss is less than in other areas of the Pacific Northwest and is controlled by organic anions rather than the bicarbonate anion.},
  annotation = {\_eprint: https://acsess.onlinelibrary.wiley.com/doi/pdf/10.2134/jeq1997.00472425002600010041x},
  copyright = {\textcopyright{} 1997 American Society of Agronomy, Crop Science Society of America, and Soil Science Society of America},
  file = {C\:\\Users\\siahl\\Zotero\\storage\\P29WKI7L\\Bockheim and Langley‚ÄêTurnbaugh - 1997 - Biogeochemical Cycling in Coniferous Ecosystems on.pdf},
  journal = {Journal of Environmental Quality},
  language = {en},
  number = {1}
}

